<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:weightSum="4"
        android:clickable="true"
        android:focusableInTouchMode="true"
        android:onClick="hideKeyboard"
        android:id="@+id/main_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true">

    <!--for room details,current time and date-->
    <RelativeLayout
            android:background="@color/room_details_background_color"
            android:layout_weight="1"
            android:id="@+id/room_details_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        <TextView
                android:id="@+id/room_name_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_24"
                android:hint="@string/room_name_hint"
                android:textColorHint="@color/white"
                android:layout_marginTop="@dimen/dp_24"
                android:textSize="@dimen/sp_22"
                android:textStyle="bold"
                android:fontFamily="@font/nunito"
                android:textColor="@color/white"
        />

        <TextView
                android:id="@+id/room_capacity"
                android:layout_below="@id/room_name_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_24"
                android:hint="@string/room_capacity_hint"
                android:textColorHint="@color/white"
                android:layout_marginTop="@dimen/dp_8"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/nunito"
                android:textColor="@color/white"
        />

        <TextView
                android:id="@+id/room_amenities"
                android:layout_alignParentBottom="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:hint="@string/amenities_hint"
                android:layout_marginTop="@dimen/dp_8"
                android:layout_marginBottom="@dimen/dp_24"
                android:textColorHint="@color/amenities_text_color"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/nunito"
                android:textColor="@color/amenities_text_color"
        />

        <TextClock
                android:textColorHint="@color/white"
                android:format24Hour="hh:mm:ss a"
                android:layout_alignParentEnd="true"
                android:id="@+id/current_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_24"
                android:layout_marginTop="@dimen/dp_24"
                android:textSize="@dimen/sp_36"
                android:fontFamily="@font/nunito"
                android:textColor="@color/white"/>

        <TextClock
                android:format24Hour="EEEE MMMM, dd"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:id="@+id/current_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColorHint="@color/white"
                android:layout_marginEnd="@dimen/dp_24"
                android:layout_marginBottom="@dimen/dp_24"
                android:textSize="@dimen/sp_18"
                android:fontFamily="@font/nunito_light"
                android:textColor="#FFFFFF"/>

    </RelativeLayout>

    <!--main layout for booking-->
    <LinearLayout
            android:orientation="horizontal"
            android:weightSum="4"
            android:layout_weight="3"
            android:visibility="visible"
            android:id="@+id/relative_main2"
            android:layout_width="match_parent"
            android:layout_height="0dp">

        <!--running and available meeting layout-->
        <RelativeLayout
                android:visibility="visible"
                android:background="@drawable/gradiant_for_available_room"
                android:id="@+id/booking_details_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:id="@+id/status_of_room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_24"
                    android:layout_marginTop="@dimen/dp_24"
                    android:textSize="@dimen/sp_36"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:textColorHint="@color/white"
                    android:hint="@string/available"/>

            <TextView
                    android:id="@+id/available_till_text_view"
                    android:layout_below="@+id/status_of_room"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_24"
                    android:layout_marginTop="@dimen/dp_32"
                    android:textSize="@dimen/sp_20"
                    android:fontFamily="@font/nunito"
                    android:textColor="#BFFFFFFF"
                    android:textColorHint="@color/white"/>



            <View
                    android:layout_below="@+id/available_till_text_view"
                    android:id="@+id/line"
                    android:background="@drawable/view_border_line"
                    android:layout_width="4dp"
                    android:layout_height="78dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="32dp"
            />
            <RelativeLayout
                    android:id="@+id/meeting_details_relative_layout"
                    android:layout_marginStart="24dp"
                    android:background="@drawable/meeting_detail_layout_border"
                    android:layout_marginTop="32dp"
                    android:layout_below="@+id/available_till_text_view"
                    android:layout_width="445dp"
                    android:layout_height="78dp">

                <TextView
                        android:id="@+id/meeting_organiser"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_16"
                        android:textSize="@dimen/sp_18"
                        android:layout_marginTop="@dimen/dp_8"
                        android:fontFamily="@font/nunito"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white"
                        android:hint="@string/booked_by_prateek_for_1_hour_and_15_minutes"/>

                <TextView
                        android:id="@+id/meeting_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="@dimen/dp_40"
                        android:layout_marginBottom="@dimen/dp_8"
                        android:textSize="@dimen/sp_18"
                        android:fontFamily="@font/nunito"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white"
                        android:hint="@string/start_end_time_hint"/>
            </RelativeLayout>
            <LinearLayout
                    android:layout_below="@+id/meeting_details_relative_layout"
                    android:layout_marginTop="@dimen/dp_32"
                    android:orientation="horizontal"
                    android:id="@+id/end_extend_action_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <TextView
                        android:id="@+id/unblock_room"
                        android:gravity="center"
                        android:layout_width="135dp"
                        android:layout_height="45dp"
                        android:text="@string/unblock"
                        android:textStyle="bold"
                        android:onClick="unBlockRoom"
                        android:visibility="gone"
                        android:background="@drawable/action_button_border"
                        android:fontFamily="@font/nunito"
                        android:textColor="@color/white"
                        android:layout_marginStart="24dp"
                        android:textSize="18sp"/>
                <TextView
                        android:id="@+id/start_meeting"
                        android:gravity="center"
                        android:layout_width="135dp"
                        android:layout_height="45dp"
                        android:text="@string/start"
                        android:textStyle="bold"
                        android:onClick="startMeeting"
                        android:visibility="gone"
                        android:background="@drawable/action_button_border"
                        android:fontFamily="@font/nunito"
                        android:textColor="@color/white"
                        android:layout_marginStart="24dp"
                        android:textSize="18sp"/>
                <TextView
                        android:id="@+id/extend_meeting"
                        android:gravity="center"
                        android:layout_width="135dp"
                        android:onClick="extendMeeting"
                        android:layout_height="@dimen/dp_48"
                        android:text="@string/extend"
                        android:visibility="gone"
                        android:textStyle="bold"
                        android:background="@drawable/action_button_border"
                        android:fontFamily="@font/nunito"
                        android:textColor="@color/white"
                        android:layout_marginStart="@dimen/dp_24"
                        android:textSize="@dimen/sp_18"/>
                <TextView
                        android:id="@+id/end_meeting"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:onClick="endMeeting"
                        android:layout_width="155dp"
                        android:layout_height="@dimen/dp_48"
                        android:text="@string/end_now"
                        android:visibility="gone"
                        android:background="@drawable/action_button_border"
                        android:fontFamily="@font/nunito"
                        android:layout_marginStart="@dimen/dp_24"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp_18"/>
            </LinearLayout>

            <LinearLayout
                    android:orientation="vertical"
                    android:background="#0F000000"
                    android:layout_marginTop="@dimen/dp_32"
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="189dp">
                <TextView
                        android:textColor="@color/white"
                        android:fontFamily="@font/nunito"
                        android:textSize="@dimen/sp_24"
                        android:text="@string/today_s_booking"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/dp_24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                <androidx.recyclerview.widget.RecyclerView
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_24"
                        android:layout_marginBottom="@dimen/dp_24"
                        android:visibility="visible"
                        android:id="@+id/recycler_view_todays_booking_list"
                        android:layout_width="match_parent"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        android:layout_height="wrap_content"
                        tools:listitem="@layout/layout_for_time_slot"/>
            </LinearLayout>





        </RelativeLayout>
    </LinearLayout>

    <!-- End meeting now-->
    <RelativeLayout
            android:visibility="gone"
            android:background="@drawable/room_details_gradiant"
            android:id="@+id/end_meeting_main_layout"
            android:layout_width="match_parent"
            android:layout_weight="3"
            android:layout_height="0dp">
        <RelativeLayout
                android:background="@color/light_color_gradiant"
                android:id="@+id/end_meeting_confirm_layout"
                android:layout_marginStart="@dimen/dp_177"
                android:layout_marginEnd="@dimen/dp_177"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:id="@+id/back"
                    android:onClick="endBack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="@string/back"
                    android:padding="@dimen/dp_16"
                    android:fontFamily="@font/nunito"
                    android:textSize="@dimen/sp_18"
                    android:textColor="@color/white"
            />
            <TextView
                    android:layout_below="@+id/back"
                    android:id="@+id/end_the_boo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_24"
                    android:layout_marginTop="@dimen/dp_48"
                    android:textSize="@dimen/sp_24"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:text="@string/end_the_booking_now"
            />
            <TextView
                    android:id="@+id/end_meeting_confirm"
                    android:gravity="center"
                    android:onClick="confirmEndMeeting"
                    android:layout_width="@dimen/dp_135"
                    android:layout_height="@dimen/dp_48"
                    android:layout_alignParentBottom="true"
                    android:text="@string/confirm"
                    android:layout_marginBottom="@dimen/dp_60"
                    android:textStyle="bold"
                    android:background="@drawable/action_button_border"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:layout_marginStart="@dimen/dp_24"
                    android:textSize="@dimen/sp_18"/>
        </RelativeLayout>


    </RelativeLayout>

    <!-- unblock meeting now-->
    <RelativeLayout
            android:visibility="gone"
            android:background="@drawable/room_details_gradiant"
            android:id="@+id/unblock_room_main_layout"
            android:layout_width="match_parent"
            android:layout_weight="3"
            android:layout_height="0dp">
        <RelativeLayout
                android:background="@color/light_color_gradiant"
                android:id="@+id/unblock_room_confirm_layout"
                android:layout_marginStart="@dimen/dp_177"
                android:layout_marginEnd="@dimen/dp_177"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:id="@+id/backUnblock"
                    android:onClick="unblockBack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="@string/back"
                    android:padding="@dimen/dp_16"
                    android:fontFamily="@font/nunito"
                    android:textSize="@dimen/sp_20"
                    android:textColor="@color/white"
            />
            <TextView
                    android:layout_below="@+id/backUnblock"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_24"
                    android:layout_marginTop="@dimen/dp_48"
                    android:textSize="@dimen/sp_28"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:text="@string/unblock_room_now"
            />
            <TextView
                    android:id="@+id/unblock_room_confirm_button"
                    android:gravity="center"
                    android:onClick="confirmUnblockRoom"
                    android:layout_width="@dimen/dp_135"
                    android:layout_height="@dimen/dp_48"
                    android:layout_alignParentBottom="true"
                    android:text="@string/confirm"
                    android:layout_marginBottom="@dimen/dp_60"
                    android:textStyle="bold"
                    android:background="@drawable/action_button_border"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:layout_marginStart="@dimen/dp_24"
                    android:textSize="@dimen/sp_20"/>
        </RelativeLayout>


    </RelativeLayout>


    <!--feedback layout-->
    <RelativeLayout
            android:visibility="gone"
            android:background="@drawable/gradiant_for_feedback"
            android:id="@+id/feedback_main_layout"
            android:layout_weight="3"
            android:layout_width="match_parent"
            android:layout_height="0dp">
        <RelativeLayout
                android:background="@color/light_color_gradiant"
                android:id="@+id/feedback_confirm_layout"
                android:layout_marginStart="@dimen/dp_177"
                android:layout_marginEnd="@dimen/dp_177"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:id="@+id/got_feedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_24"
                    android:layout_marginStart="@dimen/dp_24"
                    android:textColor="@color/white"
                    android:text="@string/got_feedback"
                    android:textSize="@dimen/sp_24"
                    android:fontFamily="@font/nunito"/>

            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/dp_36"
                    android:layout_marginStart="@dimen/dp_24"
                    android:layout_below="@+id/got_feedback"
                    android:id="@+id/feedback_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                <ImageView
                        android:layout_marginStart="@dimen/dp_16"
                        android:id="@+id/img_1"
                        android:onClick="badFeedback"
                        android:src="@drawable/ic_rate_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                <ImageView
                        android:id="@+id/img_2"
                        android:layout_marginStart="@dimen/dp_64"
                        android:src="@drawable/ic_rate_3"
                        android:onClick="averageFeedback"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                <ImageView
                        android:id="@+id/img_3"
                        android:layout_marginStart="@dimen/dp_64"
                        android:src="@drawable/ic_rate_5"
                        android:onClick="goodFeedback"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
            </LinearLayout>
            <EditText
                    android:layout_below="@id/feedback_layout"
                    android:id="@+id/feedback_edit_text"
                    android:gravity="left"
                    android:layout_marginTop="@dimen/dp_48"
                    android:paddingStart="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_8"
                    android:paddingRight="@dimen/dp_16"
                    android:paddingBottom="@dimen/dp_8"
                    android:layout_marginRight="@dimen/dp_24"
                    android:layout_marginLeft="@dimen/dp_24"
                    android:textColor="#80FFFFFF"
                    android:textColorHint="#80FFFFFF"
                    android:fontFamily="@font/nunito"
                    android:textSize="@dimen/sp_18"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:hint="@string/good_feedback_placeholder"
                    android:background="@drawable/feedback_background"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_135"/>
            <TextView
                    android:id="@+id/send_feedback"
                    android:gravity="center"
                    android:layout_width="@dimen/dp_135"
                    android:layout_height="@dimen/dp_48"
                    android:layout_alignParentBottom="true"
                    android:text="@string/submit"
                    android:layout_toRightOf="@id/cancel_feedback"
                    android:onClick="submitFeedback"
                    android:layout_marginBottom="@dimen/dp_60"
                    android:textStyle="bold"
                    android:background="@drawable/action_button_border"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:layout_marginStart="@dimen/dp_24"
                    android:textSize="@dimen/sp_18"/>

            <TextView
                    android:id="@+id/cancel_feedback"
                    android:gravity="center"
                    android:layout_width="@dimen/dp_135"
                    android:layout_height="@dimen/dp_48"
                    android:layout_alignParentBottom="true"
                    android:text="@string/cancel"
                    android:onClick="cancelFeedback"
                    android:layout_marginBottom="@dimen/dp_60"
                    android:textStyle="bold"
                    android:background="@drawable/action_button_border"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:layout_marginStart="@dimen/dp_24"
                    android:textSize="@dimen/sp_18"/>
        </RelativeLayout>
    </RelativeLayout>


    <!--Book new meeting-->
    <RelativeLayout
            android:background="@drawable/gradiant_for_available_room"
            android:id="@+id/book_now_main_layout"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_weight="3"
            android:layout_height="0dp">
        <RelativeLayout
                android:background="@color/light_color_gradiant"
                android:id="@+id/book_now_layout"
                android:layout_marginStart="@dimen/dp_177"
                android:layout_marginEnd="@dimen/dp_177"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:id="@+id/book_back"
                    android:gravity="center"
                    android:padding="@dimen/dp_16"
                    android:drawablePadding="@dimen/dp_16"
                    android:onClick="bookBack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginStart="@dimen/dp_8"
                    android:text="@string/back"
                    android:fontFamily="@font/nunito"
                    android:textSize="@dimen/sp_18"
                    android:textColor="@color/white"
            />
            <TextView
                    android:layout_below="@id/book_back"
                    android:id="@+id/passcode_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_24"
                    android:text="@string/passcode"
                    android:layout_marginTop="@dimen/dp_16"
                    android:fontFamily="@font/nunito"
                    android:textSize="@dimen/sp_24"
                    android:textColor="@color/white"
            />

            <EditText
                    android:background="@drawable/passcode_background"
                    android:layout_marginStart="@dimen/dp_24"
                    android:id="@+id/passcode_edit_text"
                    android:textColor="@color/white"
                    android:textSize="@dimen/sp_18"
                    android:letterSpacing="2"
                    android:inputType="textPassword"
                    android:paddingStart="@dimen/dp_24"
                    android:maxLength="6"
                    android:fontFamily="@font/nunito"
                    android:layout_below="@+id/passcode_label"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_width="@dimen/dp_456"
                    android:layout_height="@dimen/dp_49"/>

            <TextView
                    android:id="@+id/passcode_error_message"
                    android:layout_below="@id/passcode_edit_text"
                    android:layout_width="wrap_content"
                    android:textColor="@color/error_color"
                    android:layout_marginStart="@dimen/dp_350"
                    android:visibility="invisible"
                    android:text="@string/field_cant_be_empty"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:layout_marginTop="@dimen/dp_32"
                    android:layout_marginStart="@dimen/dp_32"
                    android:text="@string/start_time"
                    android:textColor="@color/white"
                    android:fontFamily="@font/nunito"
                    android:layout_below="@+id/passcode_edit_text"
                    android:id="@+id/start_time_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <TextView
                    android:layout_marginTop="@dimen/dp_32"
                    android:layout_marginStart="@dimen/dp_380"
                    android:text="@string/end_time"
                    android:textColor="@color/white"
                    android:fontFamily="@font/nunito"
                    android:layout_below="@+id/passcode_edit_text"
                    android:id="@+id/end_time_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <LinearLayout
                    android:id="@+id/duration_min_layout"
                    android:layout_below="@+id/start_time_label"
                    android:weightSum="3"
                    android:layout_marginStart="@dimen/dp_24"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <TextView
                        android:background="@drawable/passcode_background"
                        android:hint="@string/time_hint"
                        android:textColorHint="@color/white"
                        android:padding="@dimen/dp_16"
                        android:drawableStart="@drawable/ic_meeting_time_new"
                        android:drawablePadding="@dimen/dp_16"
                        android:fontFamily="@font/nunito"
                        android:textSize="@dimen/sp_18"
                        android:textColor="@color/white"
                        android:id="@+id/start_time_text_view"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"/>
                <TextView
                        android:background="@drawable/passcode_background"
                        android:textColorHint="@color/white"
                        android:onClick="endTimePicker"
                        android:padding="@dimen/dp_16"
                        android:drawablePadding="@dimen/dp_16"
                        android:drawableStart="@drawable/ic_meeting_time_new"
                        android:layout_marginStart="@dimen/dp_64"
                        android:fontFamily="@font/nunito"
                        android:textSize="@dimen/sp_18"
                        android:textColor="@color/white"
                        android:id="@+id/end_time_text_view"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"/>
            </LinearLayout>
            <TextView
                    android:layout_marginStart="330dp"
                    android:text="@string/field_cant_be_empty"
                    android:textColor="@color/error_color"
                    android:fontFamily="@font/nunito"
                    android:visibility="gone"
                    android:layout_below="@+id/duration_min_layout"
                    android:id="@+id/end_time_error_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:id="@+id/book_confirm"
                    android:gravity="center"
                    android:onClick="confirmBookMeeting"
                    android:layout_width="@dimen/dp_244"
                    android:layout_height="@dimen/dp_48"
                    android:layout_alignParentBottom="true"
                    android:text="@string/confirm_booking"
                    android:layout_marginBottom="@dimen/dp_60"
                    android:textStyle="bold"
                    android:background="@drawable/action_button_border"
                    android:fontFamily="@font/nunito"
                    android:textColor="@color/white"
                    android:layout_marginStart="@dimen/dp_24"
                    android:textSize="@dimen/sp_18"/>

        </RelativeLayout>


    </RelativeLayout>


    <!--Extend Meeting-->
    <RelativeLayout
            android:background="@drawable/room_details_gradiant"
            android:id="@+id/extend_main_layout"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_weight="3"
            android:layout_height="0dp">
        <RelativeLayout
                android:background="@color/light_color_gradiant"
                android:id="@+id/extend_layout"
                android:layout_marginStart="@dimen/dp_177"
                android:layout_marginEnd="@dimen/dp_177"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:id="@+id/extend_back"
                    android:gravity="center"
                    android:onClick="extendBack"
                    android:padding="@dimen/dp_16"
                    android:layout_width="wrap_content"
